{
  "name": "@font-preview/extension",
  "version": "2.2.1",
  "private": true,
  "scripts": {
    "build": "webpack --mode production --devtool eval --env debug=false",
    "format": "prettier --write './**/*{.js,.ts}'",
    "lint": "eslint './**/*{.js,.ts}'",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "test:compile": "tsc -p ./",
    "watch": "webpack --watch --mode development --env debug=true"
  },
  "devDependencies": {
    "@font-preview/eslint-base-config": "*",
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode-webview": "^1.57.0",
    "@types/wawoff2": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-webpack-plugin": "^3.2.0",
    "glob": "^7.1.6",
    "html-loader": "^2.1.2",
    "mocha": "^10.0.0",
    "prettier": "^2.3.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.6.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@font-preview/shared": "*",
    "js-yaml": "^4.1.0",
    "wawoff2": "^2.0.1",
    "zod": "^3.22.4"
  }
}
